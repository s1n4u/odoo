<odoo>

    <record id="species_dog" model="vet.species">
        <field name="name">Dog</field>
    </record>

    <record id="species_cat" model="vet.species">
        <field name="name">Cat</field>
    </record>

    <record id="species_bird" model="vet.species">
        <field name="name">Bird</field>
    </record>

    <record id="species_other" model="vet.species">
        <field name="name">Other</field>
    </record>

    <record id="breed_labrador" model="vet.breed">
        <field name="name">Labrador Retriever</field>
        <field name="species_id" ref="species_dog"/>
    </record>

    <record id="breed_germanshepherd" model="vet.breed">
        <field name="name">German Shepherd</field>
        <field name="species_id" ref="species_dog"/>
    </record>

    <record id="breed_siamese" model="vet.breed">
        <field name="name">Siamese</field>
        <field name="species_id" ref="species_cat"/>
    </record>

    <record id="breed_bengal" model="vet.breed">
        <field name="name">Bengal</field>
        <field name="species_id" ref="species_cat"/>
    </record>

    <record id="breed_parrot" model="vet.breed">
        <field name="name">Parrot</field>
        <field name="species_id" ref="species_bird"/>
    </record>

    <record id="breed_canary" model="vet.breed">
        <field name="name">Canary</field>
        <field name="species_id" ref="species_bird"/>
    </record>

    <record id="breed_hamster" model="vet.breed">
        <field name="name">Hamster</field>
        <field name="species_id" ref="species_other"/>
    </record>

    <record id="breed_rabbit" model="vet.breed">
        <field name="name">Rabbit</field>
        <field name="species_id" ref="species_other"/>
    </record>

</odoo>
